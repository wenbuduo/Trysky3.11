.checkInput{
  margin-top:50%; 
  text-align: center; 
  color: #287787;
  background: linear-gradient(90deg,transparent,rgba(222, 62, 54, 0.568),transparent);
  padding: 5px;
  font-size: 20px;
  font-weight: 800;
}

.flower{
  position: absolute;
  width: 25px;
  height: 25px;
  top: 0;
  animation-name: float;
  animation-duration: 20s;
  animation-iteration-count: infinite;
  animation-timing-function: ease-in-out;
}

@keyframes float{
  from{
    top: -25px;
    left: 0;
    transform:rotateX(30deg);
  }
  
  35%{
    left:100%;
    transform:rotateX(90deg);
  }
  60%{
    left: -25px;
    transform:rotateX(45deg);
  }
  to{
    top:100%;
    left:100%;
    transform:rotateX(180deg);
  }
}

.buttonLogin{
  margin-top:50%; 
}
.buttonLogin button{
  margin: 10px;
  color: salmon;
  font-weight: 100;
  font-size: 15px;
  background: linear-gradient(90deg,transparent 0%,rgb(255, 255, 255),transparent);
}
.buttonLogin button:active{
  background:rgba(255, 255, 255, 0.637);
}


.user{
  margin: 10px;
  position: absolute;
  right: 0px;
  
  display: flex;
  justify-content: center;
  align-items: center;
}

.head{
  width: 80px;
  height: 80px;
  margin: 10px;
  margin-left: 30px;
  /* border-radius:40px;  */
  opacity:0.90;
}


.textName{
  color: skyblue;
  border-right:1px solid #142078;
  height: 30px;  
  padding-right: 25px;
}
.textIntro{
  color: salmon;
  position: absolute;
  top: 55px;
  right: 145px;
  font-size: 10px;
}


.userBox{
  position: absolute;
  margin-top: 100px;
}
.userBox image{
  width: 25px;
  height: 25px;
  padding: 3px;
}
.userBox view{
  position: relative;
  left: -120px;
  margin-bottom: 10px;
  font-weight: 300;
  font-size: 20px;
  
  padding: 5px;
  padding-left:20px; 
  width: 120px;
  background-color: rgba(0, 100, 0, 0.116);
  border-radius: 4px;

  display: flex;
  justify-content: center;
  
}
.userBox view:active{
  transition-duration: 100ms;
  transform: translateX(10px);
}
@keyframes userKey{
  to{
    left: 0px;
  }
}

.userGet{
  animation-name: userKey;
  animation-duration: 1s;
  animation-fill-mode: forwards;
}
.userActivities{
  animation-name: userKey;
  animation-delay: 0.5s;
  animation-duration: 1s;
  animation-fill-mode: forwards;
}
.userExit{
  animation-name: userKey;
  animation-delay: 1s;
  animation-duration: 1s;
  animation-fill-mode: forwards;
}




