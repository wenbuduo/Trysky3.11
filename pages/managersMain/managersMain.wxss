page {
  margin: 0px;
  padding: 0px;
  background-image: linear-gradient(180deg, #CCFFFF 0%, rgba(234, 204, 248, 0.493) 65%, rgba(40, 238, 33, 0.151) 100%);
  background-attachment:fixed;
}

.managerNum{
  position: absolute;
  margin-top:80px;
  left: 60%;
  font-size: 100px;
  font-style: oblique;
  font-weight: 100;
  filter: blur(5px);
  color: slateblue;
}

.manager{
  display: flex;
  justify-content: center; 
}
.managerDor{
  width: 80px;
  height: 80px;
  padding-top: 5px;
  border: 1px solid #142078;
  border-radius:50%; 
  margin-top: 100px;
  box-shadow: 0 5px 20px rgba(0, 0, 0, 0.1);
  animation-name: manager;
  animation-duration: 10s;
  animation-iteration-count: infinite;
  animation-timing-function: linear;
}
.nameDor{
  position: absolute;
  margin-top:65px;
  color:   slateblue;
  font-weight: 800;
  font-size: 10px;
}
.Num{
  position: absolute;
  margin-top:80px;
  color:   slateblue;
  font-weight: 800;
  font-size: 10px;
}
@keyframes manager{
  from{
    transform: rotateY(0deg);
  }
  to{
    transform: rotateY(360deg);
  }
}

.blockButton{
