}

.buling{
  transform: translateY(-10px);
  transition: 0.3s;
}

.list1 view navigator::before{
  content: '';
  position: absolute;
  top:0;
  left: 0;
  width: 20px;
  height: 100%;
  background: rgba(255, 255, 255, 0.5);
  transform: skewX(45deg)translateX(85px);
  transition: 0.9s;
}

.list1 view navigator:hover::before{
  transform: skewX(45deg) translateX(-60px);
}

